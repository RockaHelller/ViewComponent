@model CategoryCreateVM
@{
    ViewData["Title"] = "Create";
}

<div class="container my-5">
    <form method="post" asp-action="Create">
        <input type="text" asp-for="Name" class="form-control"></input>
        <span class="text-danger d-block" asp-validation-for="Name"></span>
        <button type="submit" class="btn btn-success btn-rounded btn-fw mt-3">Submit</button>
        <a asp-action="Index" type="submit" class="btn btn-primary btn-rounded btn-fw mt-3">Back</a>
        
    </form>
</div>

